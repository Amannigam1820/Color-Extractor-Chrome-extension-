{
    "name":"Color Extracrtor",
    "description":"A color extractor extension",
    "version":"1.0.0",
    "manifest_version":3,
    "background":{
        "service_worker":"background.js"
    },
    "permissions":[
        "storage",
        "activeTab",
        "scripting"
    ],
    "action":{
        "default_popup":"popup.html"
    },
    "icons":{
        "16":"/img/picker_16.png",
        "32":"/img/picker_32.png",
        "48":"/img/picker_48.png"
    }

}